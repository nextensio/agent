  platform :macos, '11.1'
  inherit! :search_paths
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  # Pods for NextensioAppMacOSX
  pod 'OktaOidc'
  pod 'OktaAuthSdk'
  pod 'OktaJWT'
end
